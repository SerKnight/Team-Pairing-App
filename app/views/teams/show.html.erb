<p id="notice"><%= notice %></p>

<p>
  <h3>Team: <strong><%= @team.name %></strong></h3>
  <h6>Description: <strong><%= @team.description %></strong></h6>
  <% @team.members.each do |member| %>
    <h6>Members:</h6>
    <%= member.name %>
  <% end %>
</p>
<%= link_to 'Edit', edit_team_path(@team), class: "button small" %> |
<td><%= link_to 'Destroy', team_path, method: :delete, data: { confirm: 'Are you sure?' }, class: "alert button small" %></td> 
<br>
<hr>
<br>
<%= link_to 'Back', teams_path, class: "button" %>


        